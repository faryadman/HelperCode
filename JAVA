 public DataReport(String start, String end) throws ParseException {
        this.start = start;
        this.end = end;
        if (this.start !=null && this.end!=null) {
            DateFormat formatter = new SimpleDateFormat("HH:mm:ss");
            java.sql.Time timeValue1 = new java.sql.Time(formatter.parse(this.start).getTime()); //start="08:12:45"
            java.sql.Time timeValue2 = new java.sql.Time(formatter.parse(this.end).getTime());//end="12:55:00"

            LocalTime from = timeValue1.toLocalTime();
            LocalTime to = timeValue2.toLocalTime();
            Duration d = Duration.between(from, to);
            HourMinuteActivity=(String.valueOf(d.toHoursPart()+":"+d.toMinutesPart()));//d= "4:42"
        }
    }
            
            
            
